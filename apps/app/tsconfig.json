{
  "compilerOptions": {
    "paths": {
      "@styles/*": [
        "./styles/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@supabase/*": [
        "./supabase/*"
      ],
      "@backblaze/*": [
        "./backblaze/*"
      ],
      "@lib/*": [
        "lib/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "next.config.js",
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "tsconfig/nextjs.json"
}